<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/float_button_shadow"></item>
    <item android:state_pressed="true" >
        <shape android:shape="oval">

            <solid
                android:color="#fec10e" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <gradient
                android:startColor="#fec10e"
                android:endColor="#fec10e"
                android:angle="270" />
        </shape>
    </item>
</layer-list>